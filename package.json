    "bcryptjs": "2.4.3",
    "jsonwebtoken": "9.0.1",
    "serverless-dynamodb-local": "0.2.35",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@serverless/typescript": "3.30.1",
    "@types/aws-lambda": "8.10.119",
    "@types/chai": "4.3.5",
    "@types/jsonwebtoken": "9.0.2",
    "@types/mocha": "10.0.1",
    "@types/node": "20.4.2",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
